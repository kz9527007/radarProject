
cd ~/radarProject/

f = linspace(0,1, 1000);
figure(100);
plot(f,dlFeedback(0.25,f));
hold on;
plot(f,dlFeedback(0.50,f));
plot(f,dlFeedback(0.75,f));

function [resps] = dlFeedback(K, f)

T = 1;
w = 2*pi*f;
nume = 2 * (1 - cos(w*T));
K = 0.25;
deno = (1+(K*K))-(2*K*cos(w*T));
resps = nume ./ deno;

end